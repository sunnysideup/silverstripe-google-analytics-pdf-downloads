{
    "name": "sunnysideup/google-analytics-pdf-downloads",
    "description": "Measure directly accessed PDF Downloads assets through Google Analytics .",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "pdf",
        "download",
        "analytics",
        "google analytics",
        "direct access"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\GAPDFDownloads\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/npm-build-google-analytics-pdf-downloads.sh",
        "bin/npm-watch-google-analytics-pdf-downloads.sh"
    ],
    "authors": [
        {
            "name": "Si√¥n Madden",
            "email": "sion@sunnysideup.co.nz",
            "role": "lead developer"
        },
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "reviewing developer"
        }
    ]
}
